<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER" spacing="2.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="deleteButton" maxHeight="25.0" maxWidth="25.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0" HBox.hgrow="NEVER">
         <HBox.margin>
            <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
         </HBox.margin>
      </Button>
      <TextField fx:id="keyTextField" accessibleText="Key" HBox.hgrow="ALWAYS" />
      <TextField fx:id="valueTextField" accessibleText="Value" HBox.hgrow="ALWAYS" />
   </children>
</HBox>
